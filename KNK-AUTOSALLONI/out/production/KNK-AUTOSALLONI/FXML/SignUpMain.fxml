<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="430.0" prefWidth="672.0" xmlns="http://javafx.com/javafx/8.0.241" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.SignUpController">
   <children>
      <TextField fx:id="signupemri" layoutX="243.0" layoutY="67.0" promptText="Name" />
      <TextField fx:id="signupimella" layoutX="243.0" layoutY="215.0" promptText="Email" />
      <Button fx:id="singupbtn1" layoutX="239.0" layoutY="370.0" mnemonicParsing="false" onAction="#signupaction" text="SignUp" />
      <Button fx:id="loginbtn1" layoutX="359.0" layoutY="370.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <PasswordField fx:id="signuppw" layoutX="243.0" layoutY="272.0" promptText="Password" />
      <TextField fx:id="signupmbiemri" layoutX="243.0" layoutY="118.0" promptText="Surname" />
      <RadioButton fx:id="signupmale" layoutX="243.0" layoutY="337.0" mnemonicParsing="false" text="Male" />
      <RadioButton fx:id="signupfemale" layoutX="345.0" layoutY="337.0" mnemonicParsing="false" text="Female" />
      <TextField fx:id="signuplokacioni" layoutX="243.0" layoutY="167.0" promptText="Location" />
      <Label fx:id="emailerror" alignment="CENTER" layoutX="232.0" layoutY="248.0" prefHeight="18.0" prefWidth="207.0" textFill="RED" />
